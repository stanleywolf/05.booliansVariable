using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _05.booleansVariable
{
    class Program
    {
        static void Main(string[] args)
        {
            bool boolians = Convert.ToBoolean(Console.ReadLine());
            if (boolians==true)
            {
                Console.WriteLine("Yes");
            }
            else if(boolians==false)
            {
                Console.WriteLine("No");
            }
        }
    }
}
